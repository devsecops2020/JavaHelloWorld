pipeline {
    agent any
    tools {
        apache-maven:'apache-maven-3.6.3'
          }
    stages {
        stage('Build') {
            steps {
                sh 'printenv'
                withMaven(mavenSettingsConfig: 'maven-settings-global') {
                    sh 'mvn clean package'
                }
            }
        }     
    }    
}

