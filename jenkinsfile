pipeline {
    agent any
    tools {
        maven 'apache-maven-3.6.3'
        jdk 'jdk-11.0.5'
    }
    stages {
        stage('Build') {
            steps {
                sh 'printenv'
                withMaven(mavenSettingsConfig: 'maven-settings-global') {
                    sh 'mvn clean package'
                }
            }
        }     
    }    
}

