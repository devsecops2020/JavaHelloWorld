pipeline{
      agent any
      stages{
            stage("Github Checkout"){
		steps{
		    CredentialsId: git 'https://github.com/devsecops2020/JavaHelloWorld'
		      }  
            	}
            stage("Compile Stage"){
              steps{
		  withMaven(maven'apache-maven-3.6.3'){
                  sh "mvn clean compile"
                  }
              }
            }
            stage("Testing Stage"){
              steps{
		  withMaven(maven'apache-maven-3.6.3'){
                  sh "mvn test"
                  }
              }
            }
            stage("Deployment Stage"){
              steps{
		  withMaven(maven'apache-maven-3.6.3'){
                  sh "mvn deploy"
                  }
              }
           }
         }
     }

