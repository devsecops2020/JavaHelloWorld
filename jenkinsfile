pipeline{
      agent any
      stages{
            stage("Github Checkout"{
		steps{
		    git 'https://github.com/devsecops2020/JavaHelloWorld.git'
		      }  
            }
            stage("Compile Stage"){
              steps{
                  withMaven(maven:'maven-3_6_3'){
                  sh 'mvn clean compile'
                  }
              }
            }
            stage("Testing Stage"){
              steps{
                  withMaven(maven:'maven-3_6_3'){
                  sh 'mvn test'
                  }
              }
            }
            stage("Deployment Stage"){
              steps{
                  withMaven(maven:'maven-3_6_3'){
                  sh 'mvn deploy'
                  }
              }
           }
         }
     }

